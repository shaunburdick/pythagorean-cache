{
  "name": "pythagorean-cache",
  "version": "1.0.0",
  "description": "A cache that empties when full or at an interval",
  "main": "dist/index.js",
  "files": [
    "dist/"
  ],
  "scripts": {
    "test": "npm run lint && npm run test:unit",
    "test:unit": "jest",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "build": "npm run build:clean && npm run build:transpile",
    "build:clean": "rimraf dist/*",
    "build:transpile": "tsc --project tsconfig.build.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shaunburdick/pythagorean-cache.git"
  },
  "keywords": [
    "cache",
    "interval",
    "pythagorean",
    "dump"
  ],
  "author": "Shaun Burdick <npm@shaunburdick.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/shaunburdick/pythagorean-cache/issues"
  },
  "homepage": "https://github.com/shaunburdick/pythagorean-cache#readme",
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.2",
    "eslint": "^9.17.0",
    "eslint-config-shaunburdick": "^3.0.0",
    "jest": "^29.7.0",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.2.5",
    "typescript": "^5.7.2"
  }
}
