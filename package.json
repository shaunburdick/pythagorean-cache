  "license": "MIT",
  "bugs": {
    "url": "https://github.com/shaunburdick/pythagorean-cache/issues"
  },
  "homepage": "https://github.com/shaunburdick/pythagorean-cache#readme",
  "devDependencies": {
    "@types/jest": "^26.0.13",
    "@types/node": "^14.6.4",
    "coveralls": "^3.1.0",
    "jest": "^26.4.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.3.0",
    "typescript": "^4.0.2"
  }
